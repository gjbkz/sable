{
    "name": "sable",
    "version": "0.4.6",
    "description": "Starts a server and a file watcher",
    "author": {
        "name": "Kei Ito",
        "email": "kei.itof@gmail.com",
        "url": "https://github.com/kei-ito"
    },
    "license": "MIT",
    "engines": {
        "node": ">=8"
    },
    "repository": "kei-ito/sable",
    "main": "./lib/index.js",
    "bin": {
        "sable": "./bin/sable"
    },
    "scripts": {
        "lint": "eslint bin/sable lib test",
        "test": "tap --coverage test/*.js"
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "chokidar": "^3.0.1",
        "commander": "^2.20.0",
        "ws": "^7.0.0"
    },
    "devDependencies": {
        "@nlib/lint": "^3.10.0",
        "browserstack-local": "^1.4.0",
        "eslint": "^5.16.0",
        "selenium-webdriver": "4.0.0-alpha.1",
        "tap": "^14.2.2"
    },
    "renovate": {
        "extends": [
            ":ignoreModulesAndTests",
            ":unpublishSafe",
            ":semanticPrefixChore",
            ":prNotPending",
            ":prConcurrentLimit10",
            ":prHourlyLimitNone",
            ":label(dependencies)",
            ":automergeMinor"
        ]
    }
}
